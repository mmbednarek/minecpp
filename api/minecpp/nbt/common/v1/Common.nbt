syntax = "nbt";
package minecpp.nbt.common.v1;

message ChunkPosition {
  int32 x = 1;
  int32 y = 2;
}

message BlockPosition {
  int32 x = 1;
  int32 y = 2;
  int32 z = 3;
}

message FaceMask {
    int8 down = 1;
    int8 east = 2;
    int8 north = 3;
    int8 south = 4;
    int8 up = 5;
    int8 west = 6;
}
