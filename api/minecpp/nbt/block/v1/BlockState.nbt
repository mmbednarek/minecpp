syntax = "nbt";
package minecpp.nbt.block.v1;

import "minecpp/nbt/common/v1/Common.nbt";

message BlockState {
    int8 blocks_movement = 1 [label = "BlocksMovement"];
    int32 luminance = 2 [label = "Luminance"];
    int32 opacity = 3 [label = "Opacity"];
    common.v1.FaceMask solid_faces = 4 [label = "SolidFaces"];
}
